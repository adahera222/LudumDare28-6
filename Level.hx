        Main.instance.tiles.push(newTile);
        Main.instance.addChild(newTile);
        Main.instance.addChild(Main.instance.player);
    }

    public function changeMap(id:Int){
        this.id = id;
        Main.instance.clearTiles();
        for(i in 0...maps[id].length){
            var tile = new Tile(maps[id][i]);
            var x = Math.floor(i % Main.WIDTH);
            var y = Math.floor(i / Main.WIDTH);
            tile.setLoc(x, y);
            Main.instance.tiles.push(tile);
        }

        Main.instance.changeLevel();
    }
